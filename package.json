{
  "name": "carol",
  "version": "0.0.1",
  "private": true,
  "main": "src",
  "engines": {
    "node": ">=6.6.0"
  },
  "scripts": {
    "start": "node .",
    "test": "jest",
    "test:dev": "jest -u",
    "coverage": "npm test -- --coverage && opn coverage/lcov-report/index.html",
    "dev": "concurrently --kill-others 'nodemon --watch src' 'node webpack/webpack-dev-server'",
    "lint": "eslint 'src/**/*.js' 'src-clean/**/*.js' 'webpack/**/*.js'",
    "lint:css": "stylelint 'src/components/**/*.js'",
    "clean": "del 'dist/!(.git*|Procfile)**'",
    "copy": "copyfiles -u 1 public/* public/**/* dist",
    "build": "npm run clean && npm run copy && cross-env NODE_ENV=production webpack --config webpack/webpack.config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quotatis/carol.git"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "^.+\\.(css|scss|less)$": "<rootDir>/test/styleMock.js",
      "^(components|containers)$": "<rootDir>/test/componentsMock.js"
    }
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-jest": "^17.0.2",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-import-resolver-webpack": "^0.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^6.3.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "jest-cli": "^17.0.3",
    "json-loader": "^0.5.4",
    "nodemon": "^1.11.0",
    "opn-cli": "^3.1.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.3.2",
    "react-test-renderer": "^15.4.1",
    "redux-mock-store": "^1.2.1",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^15.0.1",
    "stylelint-processor-styled-components": "0.0.4",
    "url-loader": "^0.5.7"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.11",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "copyfiles": "^1.0.0",
    "cross-env": "^3.1.3",
    "csurf": "^1.9.0",
    "dateformat": "^2.0.0",
    "del-cli": "^0.2.0",
    "enzyme": "^2.4.1",
    "expect": "^1.20.1",
    "express": "^4.14.0",
    "express-force-ssl": "^0.3.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "google-maps-react": "^1.0.19",
    "html-webpack-plugin": "^2.24.1",
    "immutable": "^3.8.1",
    "intl": "^1.2.5",
    "isomorphic-fetch": "^2.2.1",
    "jsuri": "^1.3.1",
    "lodash": "^4.16.4",
    "material-ui": "^0.16.6",
    "moment": "^2.17.1",
    "morgan": "^1.7.0",
    "path": "^0.12.7",
    "raw-loader": "^0.5.1",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-cookie": "^1.0.4",
    "react-date-picker": "^5.3.28",
    "react-dom": "^15.4.1",
    "react-dropzone": "^3.8.0",
    "react-facebook-login": "^3.4.1",
    "react-google-login": "^2.5.8",
    "react-google-recaptcha": "^0.5.4",
    "react-helmet": "^3.3.1",
    "react-hot-loader": "^3.0.0-beta.5",
    "react-intl": "^2.1.5",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "react-router-scroll": "^0.3.3",
    "react-tap-event-plugin": "^2.0.1",
    "reapop": "^0.6.1",
    "reapop-theme-wybo": "^0.4.0",
    "redux": "^3.6.0",
    "redux-form": "^6.4.1",
    "redux-form-submit": "^0.1.2",
    "redux-saga": "^0.13.0",
    "redux-thunk": "^2.1.0",
    "serialize-javascript": "^1.3.0",
    "styled-components": "^1.0.7",
    "validator": "^6.2.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "webpack-isomorphic-tools": "^2.6.3"
  }
}
