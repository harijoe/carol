    "react-tap-event-plugin": "^2.0.1",
    "reapop": "^1.1.1",
    "redux": "^3.6.0",
    "redux-form": "^6.4.1",
    "redux-form-submit": "^0.1.2",
    "redux-saga": "^0.15.4",
    "redux-thunk": "^2.1.0",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.4.2",
    "styled-components": "^2.0.0",
    "uuid": "^3.0.1",
    "validator": "^7.0.0",
    "webpack": "^3.1.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-isomorphic-tools": "^3.0.1"
  }
}
