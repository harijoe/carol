{
  "name": "carol",
  "version": "1.4",
  "private": true,
  "main": "src",
  "scripts": {
    "start": "node .",
    "start:mocks": "concurrently \"yarn simulado:start\" \"yarn simulado:prime\" \"NODE_ENV=mocks yarn start\" ",
    "test": "jest",
    "coverage": "yarn test -- --coverage && opn coverage/lcov-report/index.html",
    "purge": "babel-node scripts/purge.js",
    "lint": "eslint .",
    "lint:css": "stylelint 'src/components/**/*.js'",
    "clean": "rm -rf dist/ webpack/webpack-assets.json",
    "build": "yarn run clean && webpack --config webpack",
    "build:production": "cross-env NODE_ENV=production yarn build -- --display-optimization-bailout",
    "build:bench:sizemap": "yarn run clean && cross-env NODE_ENV=production webpack --config webpack/bench.config",
    "build:bench:gzipped": "cross-env NODE_ENV=production yarn build && tar -czf dist/gzipped.tar.gz dist/main.*.js && ls -l dist/gzipped.tar.gz",
    "postinstall": "yarn webpack-assets:create",
    "prettier": "find . -name '*.js' | grep -v node_modules | grep -vE 'i18n/(en|es|fr).js' | xargs yarn prettier:single --",
    "prettier:single": "prettier --no-semi  --trailing-comma=all --single-quote --print-width 140 --list-different --write",
    "webpack-assets:create": "node scripts/ensure-webpack-assets.js",
    "webpack-assets:refresh": "yarn run clean && yarn webpack-assets:create",
    "storybook": "start-storybook -s .storybook/public -p 9001",
    "build-storybook": "build-storybook -s .storybook/public -o .storybook/public",
    "simulado:start": "simulado",
    "simulado:prime": "node scripts/simulado-prime.js",
    "cucumber": "cross-env NODE_ENV=mocks nightwatch",
    "i18n": "babel-node ./scripts/i18n.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quotatis/carol.git"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "^.+\\.(css|scss|less)$": "<rootDir>/test/styleMock.js",
      "^(components|containers)$": "<rootDir>/test/componentsMock.js",
      "^(store|\\.\\.)\\/selectors$": "<rootDir>/test/selectorsMock.js",
      "^(store|\\.\\.)\\/actions$": "<rootDir>/test/actionsMock.js"
    },
    "setupFiles": [
      "<rootDir>/test/matchMediaMock.js"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.1.8",
    "@storybook/addon-links": "^3.1.6",
    "@storybook/components": "^3.1.5",
    "@storybook/react": "^3.1.8",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-styled-components-named": "^0.1.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "chromedriver": "^2.31.0",
    "clean-webpack-plugin": "^0.1.15",
    "cloudinary": "^1.9.0",
    "colors": "^1.1.2",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.4",
    "cucumber": "^3.0.1",
    "enhanced-resolve": "^3.1.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-prettier": "^2.3.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "foreman": "^2.0.0",
    "google-spreadsheet": "^2.0.4",
    "html-loader": "^0.4.4",
    "jest-cli": "^20.0.4",
    "json-loader": "^0.5.4",
    "nightwatch": "^0.9.16",
    "nightwatch-cucumber": "^8.0.10",
    "opn-cli": "^3.1.0",
    "prettier": "^1.5.3",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.4.1",
    "react-test-renderer": "^15.4.1",
    "redux-mock-store": "^1.2.1",
    "redux-saga-test-plan": "^3.1.0",
    "selenium-server": "^3.5.0",
    "selenium-webdriver": "^3.4.0",
    "simulado": "^2.8.1",
    "style-loader": "^0.18.2",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-processor-styled-components": "^0.1.0",
    "url-loader": "^0.5.8",
    "webpack-bundle-analyzer": "^2.8.2"
  },
  "dependencies": {
    "algoliasearch": "^3.24.0",
    "anchorate": "^1.1.0",
    "babel-core": "^6.17.0",
    "babel-polyfill": "^6.16.0",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "copyfiles": "^1.0.0",
    "cross-env": "^5.0.1",
    "csurf": "^1.9.0",
    "dateformat": "^2.0.0",
    "expect": "^1.20.1",
    "express": "^4.14.0",
    "express-force-ssl": "^0.3.2",
    "history": "^3.0.0",
    "intl": "^1.2.5",
    "isomorphic-fetch": "^2.2.1",
    "jsuri": "^1.3.1",
    "lodash": "^4.16.4",
    "markdown-it": "^8.4.0",
    "method-override": "^2.3.9",
    "morgan": "^1.7.0",
    "prop-types": "^15.5.8",
    "raven-js": "^3.17.0",
    "react": "^15.6.1",
    "react-cookie": "^1.0.5",
    "react-dom": "^15.6.1",
    "react-dropzone": "^3.8.0",
    "react-facebook-login": "^3.6.2",
    "react-google-login": "^2.8.0",
    "react-google-tag-manager": "^2.1.0",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "^3.0.0-beta.5",
    "react-intl": "^2.3.0",
    "react-motion": "^0.5.0",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.5",
