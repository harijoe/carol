{
  "presets": ["es2015", "react", "stage-1"],
  "plugins": [
    "styled-components-named",
    "react-hot-loader/babel",
    "dynamic-import-node"
  ],
  "env": {
    "production": {
      "presets": [["es2015", { "modules": false }], "react", "stage-1"],
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    }
  }
}
