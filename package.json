{
  "name": "carol",
  "version": "1.12.3",
  "private": true,
  "main": "src",
  "scripts": {
    "start": "node .",
    "start:mocks": "concurrently \"yarn simulado:start\" \"yarn simulado:prime\" \"NODE_ENV=mocks yarn start\" ",
    "test": "jest",
    "coverage": "yarn test -- --coverage && opn coverage/lcov-report/index.html",
    "lint": "eslint .",
    "lint:css": "stylelint 'src/components/**/*.js'",
    "clean": "rm -rf dist/ webpack/webpack-assets.json",
    "build": "yarn run clean && webpack --config webpack",
    "build:production": "cross-env NODE_ENV=production yarn build -- --display-optimization-bailout",
    "build:bench:sizemap": "yarn run clean && cross-env NODE_ENV=production webpack --config webpack/bench.config",
    "build:bench:gzipped": "cross-env NODE_ENV=production yarn build && tar -czf dist/gzipped.tar.gz dist/main.*.js && ls -l dist/gzipped.tar.gz",
    "changelog": "babel-node scripts/changelog.js",
    "postinstall": "yarn webpack-assets:create",
    "prettier": "find . -name '*.js' | grep -v node_modules | grep -vE 'i18n/(en|es|fr).js' | xargs yarn prettier:single",
    "prettier:only-modified": "git diff --name-only master... | grep -E '.*\\.js$' | grep -vE 'i18n/(en|es|fr).js' | xargs find 2>/dev/null | xargs yarn prettier:single || true",
    "prettier:single": "prettier --no-semi  --trailing-comma=all --single-quote --print-width 140 --list-different --write",
    "webpack-assets:create": "node scripts/ensure-webpack-assets.js",
    "webpack-assets:refresh": "yarn run clean && yarn webpack-assets:create",
    "storybook": "start-storybook -s .storybook/public -p 9001",
    "build-storybook": "build-storybook -s .storybook/public -o .storybook/public",
    "simulado:start": "simulado",
    "simulado:prime": "babel-node scripts/simulado-prime.js",
    "cucumber": "cross-env NODE_ENV=mocks nightwatch",
    "i18n": "babel-node ./scripts/i18n.js",
    "release:pr": "babel-node scripts/create-release-pr",
    "release:create": "babel-node scripts/create-release",
    "release:sentry": "scripts/sentry-release.sh",
    "generate:components": "babel-node scripts/generate-components-exports",
    "deploy:preprod": "scripts/deploy_carol_preprod.sh",
    "deploy:prod": "scripts/deploy_carol_prod.sh",
    "algolia:content": "babel-node scripts/populate-algolia-content.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quotatis/carol.git"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "^.+\\.(css|scss|less)$": "<rootDir>/test/styleMock.js",
      "^(components|containers)$": "<rootDir>/test/componentsMock.js",
      "^(store|\\.\\.)\\/selectors$": "<rootDir>/test/selectorsMock.js",
      "^(store|\\.\\.)\\/actions$": "<rootDir>/test/actionsMock.js"
    },
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/test/matchMediaMock.js",
      "<rootDir>/test/testSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.15",
    "@storybook/addon-links": "^3.2.15",
    "@storybook/react": "^3.2.15",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components-named": "^0.1.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "btoa": "^1.1.2",
    "chromedriver": "2.33.2",
    "cloudinary": "^1.9.1",
    "colors": "^1.1.2",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "cucumber": "^3.1.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "expect": "^21.2.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "google-spreadsheet": "^2.0.4",
    "html-loader": "^0.5.1",
    "jest-cli": "^21.2.1",
    "json-loader": "^0.5.7",
    "markdown-it": "^8.4.0",
    "markdown-it-attrs": "^1.2.0",
    "markdown-it-emoji": "^1.4.0",
    "nightwatch": "^0.9.16",
    "nightwatch-cucumber": "^8.2.9",
    "prettier": "^1.8.2",
    "raf": "^3.4.0",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^16.1.1",
    "readline-sync": "^1.4.7",
    "redux-saga-test-plan": "^3.3.0",
    "rimraf": "^2.6.2",
    "selenium-server": "^3.7.1",
    "selenium-webdriver": "^3.6.0",
    "simulado": "^2.8.1",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-processor-styled-components": "^1.0.0",
    "url-loader": "^0.6.2",
    "webpack-bundle-analyzer": "^2.9.1"
  },
  "dependencies": {
    "algoliasearch": "^3.24.5",
    "anchorate": "^1.1.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "commander": "^2.11.0",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.1",
    "dateformat": "^3.0.2",
    "express": "^4.16.2",
    "express-force-ssl": "^0.3.2",
    "find": "^0.2.7",
    "history": "^3.0.0",
    "intl": "^1.2.5",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0",
    "php-unserialize": "^0.0.1",
    "prompt": "^1.0.0",
    "prop-types": "^15.6.0",
    "raven-js": "^3.20.0",
    "react": "^16.1.1",
    "react-cookie": "^1.0.5",
    "react-dom": "^16.1.1",
    "react-dropzone": "^4.2.1",
    "react-facebook-login": "^3.6.2",
    "react-google-login": "^2.11.3",
    "react-google-tag-manager": "^2.2.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.1.2",
    "react-images": "^0.5.11",
    "react-intl": "^2.4.0",
    "react-masonry-component": "^6.0.1",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-router-scroll": "^0.4.4",
    "react-scroll": "^1.6.4",
    "react-slick": "^0.15.4",
    "react-tooltip": "^3.4.0",
    "react-truncate": "^2.2.2",
    "reapop": "^1.1.4",
    "redux": "^3.7.2",
    "redux-form": "^7.1.2",
    "redux-form-submit": "^0.1.3",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.4.0",
    "serve-favicon": "^2.4.5",
    "styled-components": "^2.2.3",
    "uuid": "^3.0.1",
    "validator": "^9.1.1",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-isomorphic-tools": "^3.0.5",
    "xml2js": "^0.4.19"
  }
}
